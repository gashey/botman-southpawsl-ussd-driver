{
    "name": "gashey/botman-southpawsl-ussd-driver",
    "type": "library",
    "description": "Botman Southpawsl USSD Driver for Laravel Projects",
    "keywords": [
        "php",
        "laravel",
        "botman",
        "ussd",
        "driver"
    ],
    "homepage": "https://github.com/gashey/botman-southpawsl-ussd-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "George Kofi Hagan",
            "email": "georgekhagan@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../botman"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "gashey/botman": "2.0.x-dev"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Gashey\\BotmanSouthpawslUssdDriver\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gashey\\BotmanSouthpawslUssdDriver\\UssdServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
